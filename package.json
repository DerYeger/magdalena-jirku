    "core-js": "3.22.8",
    "nuxt": "2.15.8",
    "prism-themes": "1.9.0",
    "vue": "2.6.14",
    "vue-marmoset-viewer": "1.3.2",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@mdi/js": "5.9.55",
