{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@composables/*": ["./src/composables/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@styles/*": ["./src/styles/*"]
    },
    "skipLibCheck": true
  },
  "exclude": ["playwright-report", "dist", "node_modules"],
  "vueCompilerOptions": {
    "strictCssModules": true,
    "strictTemplates": true
  }
}
